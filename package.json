    "jasmine-spec-reporter": "~5.0.2",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.2.0",
    "pre-commit": "^1.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ]
  }
